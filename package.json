    "recompose": "^0.20.0",
    "redux": "^3.3.1",
    "redux-actions": "^0.10.0",
    "redux-form": "^5.2.3",
    "redux-localstorage": "^0.4.0",
    "redux-logger": "^2.1.4",
    "redux-saga": "^0.10.4",
    "require-hacker": "^2.1.2",
    "reselect": "^2.5.1",
    "rimraf": "^2.4.3",
    "sinon": "^1.17.3",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.0",
    "stylelint": "^6.3.2",
    "stylelint-webpack-plugin": "^0.2.0",
    "supertest": "^1.2.0",
    "supertest-as-promised": "^3.1.0",
    "testdouble": "^1.4.3",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.6.4",
    "webpack-split-by-path": "0.0.8",
    "whatwg-fetch": "^1.0.0"
  },
  "dependencies": {
    "bluebird": "^3.4.1",
    "express": "^4.13.3",
    "helmet": "^2.0.0",
    "joi": "^8.4.2",
    "monk": "2.1.0",
    "ramda": "^0.21.0",
    "winston": "^2.1.1"
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "git://github.com/boxal/boxal.git"
  },
  "bugs": {
    "url": "https://github.com/boxal/boxal/issues"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
