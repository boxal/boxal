{
  "name": "boxal",
  "version": "0.1.0",
  "description": "A boiler-plate setup for React/Redux applications",
  "license": "MIT",
  "engines": {
    "node": "6.2.0"
  },
  "scripts": {
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production webpack -p",
    "clean": "rimraf dist/ coverage/",
    "commit": "git cz",
    "cover": "nyc check-coverage --lines 80 --functions 80 --branches 80 --statements 80",
    "coveralls:report": "nyc report --reporter=text-lcov | coveralls",
    "dev:server": "cross-env NODE_ENV=development PORT=3000 nodemon --use_strict server/index.js",
    "dev:client": "cross-env NODE_ENV=development webpack-dev-server -d --inline --hot --progress --no-info",
    "dev": "concurrently -r 'npm run dev:client' 'npm run dev:server'",
    "lint": "npm run lint-js && npm run lint-css",
    "lint-css": "stylelint './src/**/*.css'",
    "lint-js": "eslint '**/*.js' --ignore-path .gitignore",
    "lint-js:fix": "npm run lint-js -- --fix",
    "postinstall": "npm run build",
    "start": "cross-env NODE_ENV=production node --use_strict server/node-server.js",
    "pretest": "npm run clean coverage/ && npm run lint",
    "test": "nyc ava",
    "posttest": "npm run cover",
    "test:watch": "ava --watch"
  },
  "ava": {
    "files": [
      "src/**/*.test.js",
      "server/**/*.test.js"
    ],
    "require": [
      "babel-register",
      "./test/entry.js"
    ],
    "babel": "inherit"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "all": true,
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "exclude": [
      "**/*.test.js",
      "test/entry.js",
      "src/index.js",
      "server/app.js",
      "server/index.js",
      "server/enable-html5-routing.js",
      "src/store/logger.js",
      "src/store/configure-store.js",
      "webpack.config.js"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^6.3.3",
    "autoprefixer-core": "^6.0.1",
    "ava": "^0.15.2",
    "babel": "^6.5.2",
    "babel-cli": "^6.8.0",
    "babel-core": "^6.4.0",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.1",
    "babel-plugin-__coverage__": "^0.111111.11",
    "babel-plugin-transform-async-to-module-method": "^6.8.0",
    "babel-plugin-transform-es2015-destructuring": "^6.9.0",
    "babel-plugin-transform-react-constant-elements": "^6.8.0",
    "babel-plugin-transform-react-inline-elements": "^6.8.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-node6": "^11.0.0",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.9.0",
    "basscss": "^8.0.1",
    "basscss-addons": "^1.0.0-beta4",
    "basscss-align": "^1.0.2",
    "basscss-basic": "^1.0.0",
    "basscss-border": "^4.0.2",
    "basscss-flexbox": "^1.0.1",
    "basscss-grid": "^2.0.0",
    "basscss-hide": "^1.0.1",
    "basscss-layout": "^3.1.0",
    "basscss-margin": "^1.0.7",
    "basscss-padding": "^1.1.3",
    "basscss-position": "^2.0.3",
    "basscss-type-scale": "^1.0.5",
    "basscss-typography": "^3.0.3",
    "body-parser": "^1.15.1",
    "chai": "^3.4.0",
    "classnames": "^2.2.3",
    "commitizen": "^2.8.2",
    "concurrently": "^2.0.0",
    "coveralls": "^2.11.9",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.0",
    "cssnano": "^3.5.2",
    "cz-conventional-changelog": "^1.1.6",
    "deepmerge": "^0.2.10",
    "denodeify": "^1.2.1",
    "enzyme": "^2.2.0",
    "eslint": "2.10.2",
    "eslint-loader": "^1.1.1",
    "eslint-plugin-immutable": "^1.0.0",
    "eslint-plugin-react": "^5.0.0",
    "file-loader": "^0.8.4",
    "history": "^2.0.1",
    "html-webpack-plugin": "^2.9.0",
    "immutable": "^3.7.5",
    "jsdom": "^9.2.1",
    "json-loader": "^0.5.3",
    "mocha": "^2.5.0",
    "node-fetch": "^1.5.3",
    "nodemon": "^1.9.2",
    "nyc": "^6.4.3",
